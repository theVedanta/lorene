  <style>
    .wrapper {
      transition: all 0.6s;
    }
    .form-box {
      pointer-events: all;
      position: fixed;
      top: 50%;
      left: 50%;
      width: 77vw;
      height: 88vh;
      overflow-y: scroll;
      transform: translate(-50%, -50%);
      padding: 2.6vh 3vw;
      border-radius: 12px;
      background: white;
      box-shadow: 0 0 50px rgba(0, 0, 0, 0.2);
      opacity: 0;
      pointer-events: none;
      transition: all 0.3s;
    }
    form h1 {
      font-size: 2vw;
      font-weight: 500;
      position: relative;
      display: inline;
      margin-bottom: 0;
    }
    form h1::after {
      content: "";
      position: absolute;
      bottom: -10px;
      left: 0;
      width: 65%;
      height: 3px;
      border-radius: 50px;
      background: #51cbce;
    }
    form input,
    form select,
    .form-control {
      border: 1px rgba(0, 0, 0, 0.438) solid;
      transition: all 0.2s;
    }
    .col {
      margin: 1.4vh 0;
    }
    .required::before {
      content: "*";
      color: red;
    }
    .form-box-active {
      opacity: 1;
      pointer-events: all;
    }
    .wrapper-active {
      filter: brightness(75%);
      pointer-events: none;
    }
    form i {
      font-size: 2.3vw;
      cursor: pointer;
      z-index: 9;
    }
    .input input,
    .input select {
      background: #f9f9f9;
      border: 1px solid #dcdcdc;
      border-radius: 9px;
      font-size: 1vw;
      color: #434175;
      padding: 1.4vh 0.8vw;
      width: 100%;
    }
    .input label {
      color: #434175;
      font-size: 1vw;
      font-weight: 500;
    }
    .input input:focus,
    .input select:focus,
    .input input:hover,
    .input select:hover {
      border: 1px solid #6c63ff;
    }
    form .btn {
      font-size: 1.2vw;
      font-weight: 600;
      text-transform: none;
    }
    .card {
      padding: 0.6vh 2vw;
      box-shadow: 0 0 20px rgba(18, 18, 19, 0.218);
      width: 99%;
      max-height: 87vh;
      overflow-y: scroll;
      overflow-x: hidden;
      margin-bottom: 1.2vh;
      height: 87vh;
    }
    .card-stats .card-body .numbers .card-category {
      font-size: 1.2vw;
      white-space: nowrap;
    }
    .card h4 {
      font-size: 3vh;
    }
    .card hr {
      margin: 1vh 0;
      width: 100%;
    }
    .table > thead > tr > th,
    .table > tbody > tr > th,
    .table > tfoot > tr > th,
    .table > thead > tr > td,
    .table > tbody > tr > td,
    .table > tfoot > tr > td {
      padding: 0.85vh 0;
      font-size: 1.1vw;
      text-transform: none;
    }
    .table > thead > tr > th,
    .table > tbody > tr > th {
      padding-top: 0;
    }
    .table-cont {
      overflow-x: hidden;
      overflow-y: scroll;
    }
    .table-responsive {
      overflow-y: scroll;
      padding: 3vh 0;
    }
    .table .btn {
      text-transform: none;
    }
    .card-head {
      display: flex;
      align-items: center;
      justify-content: space-between;
      position: sticky;
      top: 0;
    }
    .card-head .left i {
      color: #51cbce;
    }
    .card-head h4 {
      font-weight: 600;
      font-size: 1.6vw;
    }
    .card-head .right {
      display: flex;
      justify-content: center;
      align-items: center;
    }
    .card-head .input {
      display: flex;
      align-items: center;
    }
    .card-head .input {
      background-color: transparent;
      border: 2px #51cbce solid;
      padding: 0 0.8vw;
      color: #51cbce;
      font-size: 1.1vw;
      border-radius: 13px;
    }
    .card-head .input input {
      background: transparent;
      border: none;
      padding: 1vh 0.6vw;
      font-size: 1vw;
      font-weight: 500;
    }
    .card-head .input input::placeholder {
      color: #51cbce;
    }
    .card-head .btn {
      height: 100%;
      font-size: 1vw;
      text-transform: none;
      border-radius: 9px;
      margin-left: 1vw;
    }
    .btn-danger {
      background: #e34f4f;
    }
    .table td .btn-danger:hover {
      background: red;
    }
    .form-top {
      display: flex;
      justify-content: space-between;
      align-items: center;
      position: sticky;
      top: 0;
      padding: 1vh 0;
      background: #fff;
      z-index: 999;
    }
    .form-top::before {
      content: "";
      position: absolute;
      top: -100%;
      left: 0;
      width: 100%;
      height: 16vh;
      z-index: -1;
      background: #fff;
    }
    .no-data-hold {
      display: none;
      justify-content: center;
      align-items: center;
      height: 50vh;
    }
    .no-data-hold h4 {
      color: #0e5456;
      font-weight: 600;
    }
    .no-hr {
      display: none;
    }
    .upload {
        text-align: center;
        padding-bottom: 1vh;
        color: #208184;
        font-size: 1.2vw;
        font-weight: 500;
      }
      .upload a {
        color: #0e5456;
      }
      .upload a:hover {
        text-decoration: none;
      }
      .upload a i {
        color: #208184;
      }
    @media (max-width: 1100px) {
      .table > thead > tr > th,
      .table > tbody > tr > th,
      .table > tfoot > tr > th,
      .table > thead > tr > td,
      .table > tbody > tr > td,
      .table > tfoot > tr > td {
        font-size: 1vw;
      }
    }
    @media (max-width: 800px) {
      .form-box {
        width: 80vw;
        font-size: 1.6vw;
      }
      form h1 {
        font-size: 3.6vw;
      }
      .form-control {
        font-size: 1.6vw;
      }
      form i {
        font-size: 4vw;
      }
      .card {
        padding: 1vh 5vw;
      }
      .table > thead > tr > th,
      .table > tbody > tr > th,
      .table > tfoot > tr > th,
      .table > thead > tr > td,
      .table > tbody > tr > td,
      .table > tfoot > tr > td {
        font-size: 2vw;
      }
      .table-responsive {
        padding-left: 0;
        padding-right: 0;
      }
    }
    @media (max-width: 500px) {
      .form-box {
        width: 95vw;
        font-size: 2vw;
        padding: 3vh 8vw;
      }
      form h1 {
        font-size: 5.6vw;
      }
      form i {
        font-size: 6vw;
      }
      .form-control {
        font-size: 2vw;
        padding: 0.4vh 1vw;
      }
      .table > tbody > tr > th,
      .table > tfoot > tr > th,
      .table > thead > tr > th {
        font-size: 3vw;
      }
      .table-responsive {
        font-size: 3vw;
      }
      .table-responsive .btn {
        padding: 1vh 3vw;
      }
      .card {
        padding: 1.2vh 7vw;
      }
      .table > thead > tr > th {
        font-size: 2.4vw;
      }
      .table-responsive {
        padding: 1vh 0;
      }
    }
  </style>
  <body>
    <script>
      const dif = (event) => {
        const diff =
          document.querySelector("#diff").value === "diff" ? true : false;

        if (diff) {
          document.querySelector(".ship").style.display = "block";
        } else {
          document.querySelector(".ship").style.display = "none";
        }
      };
    </script>

