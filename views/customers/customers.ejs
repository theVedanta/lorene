        <div class="content">
          <!-- VERIFICATION ---------------------------------------------------------------- -->
          <% if (!user.verified) { %>
            <% if (verErr) { %>
              <div class="row">
              <div class="col">
                <div class="upload"><%= verErr %></div>
              </div>
            </div>
            <% } else if (user.bankInfo) { %>
            <div class="row">
              <div class="col">
                <div class="upload">Your documents are under verification.</div>
              </div>
            </div>
            <% } else { %>
            <div class="row">
              <div class="col">
                <div class="upload">
                  Please upload your documents
                  <a href="/account"
                    >here. &nbsp;<i class="fas fa-file-upload"></i
                  ></a>
                </div>
              </div>
            </div>
            <% } %>
          <% } %>
          <div class="row">
            <div class="col">
              <div class="card card-stats">
                <div class="card-head">
                  <div class="left">
                    <h4>
                      <i class="fas fa-user-friends"></i>&nbsp;&nbsp;Customers
                    </h4>
                  </div>
                  <div class="right">
                    <div class="input">
                      <i class="fas fa-search"></i>
                      <input type="text" placeholder="Search" id="search" />
                    </div>
                    <button id="create" class="btn btn-primary">
                      <i class="fas fa-plus"></i>&nbsp;Add
                    </button>
                  </div>
                </div>
                <div class="table-cont">
                  <div class="table-responsive">
                    <table class="table">
                      <thead class="text-primary">
                        <tr>
                          <th>S. No.</th>
                          <th>Business Name</th>
                          <th style="text-align: left">Email</th>
                          <th style="text-align: left">Mobile Number</th>
                          <th style="text-align: left">Action</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% for (let i = 0; i < customers.length; i++) { %>
                        <tr>
                          <td><%= i + 1 %></td>
                          <td class="businessName">
                            <%= customers[i].businessName %>
                          </td>
                          <td style="text-align: left">
                            <%= customers[i].mail ? customers[i].mail : "-" %>
                          </td>
                          <td style="text-align: left">
                            <%= customers[i].phone ? customers[i].phone:"-" %>
                          </td>
                          <td style="text-align: left">
                            <a
                              class="btn btn-primary"
                              href="/customers/edit/<%= customers[i].id %>"
                              ><i class="fas fa-pen"></i>&nbsp; Edit</a
                            >
                          </td>
                        </tr>
                        <% } %>
                      </tbody>
                    </table>
                    <% if (customers.length === 0) { %>
                      <hr class="no-hr" style="display: flex" />
                    <div class="no-data-hold" style="display: flex">
                      <% } else { %>
                        <hr class="no-hr" />
                      <div class="no-data-hold" style="display: none">
                        <% } %>
                        <h4 class="no-data">No data to Display</h4>
                      </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>

